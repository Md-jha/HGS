
@{
    ViewData["Title"] = "faqs";
}
@section MetaTags {
    <title> FAQ's </title>

}


<!--Page Title-->
<section class="page-banner style-two" style="background-image:url(/images/background/about-page-title-bg.jpg);">
    <div class="auto-container">
        <div class="inner-container clearfix">
            <h1>FAQ's</h1>
            <ul class="bread-crumb clearfix">
                <li><a href="/Home/Index">Home</a></li>
                <li>Faq's</li>
            </ul>
        </div>
    </div>
</section>
<!--End Page Title-->

<section class="about-industry-section">
    <div class="container">

        <div class="sec-title centered">
            <div class="title">FAQ's</div>
            <h2> Frequently asked questions</h2>
        </div>



        <div class="row">
            <div style="width: 100%; ">

                <!-- Accordion -->
                <div id="accordionExample" class="accordion shadow">

                    <!-- Accordion item 1 -->
                    <div class="card">
                        <div id="headingOne" class="card-header bg-white shadow-sm border-0">
                            <h2 class="mb-0">
                                <button type="button" data-toggle="collapse" data-target="#collapseOne" aria-expanded="false" aria-controls="collapseOne" class="btn btn-link text-dark font-weight-bold text-uppercase collapsible-link collapsed">What is a fabric printing machine used for?</button>
                            </h2>
                        </div>
                        <div id="collapseOne" aria-labelledby="headingOne" data-parent="#accordionExample" class="collapse" style="">
                            <div class="card-body p-5">
                                <p class="font-weight-light m-0">A fabric printing machine is used for printing patterns and designs directly on textiles, making it essential in digital textile fabric printing processes.</p>
                            </div>
                        </div>
                    </div><!-- End -->
                    <!-- Accordion item 2 -->
                    <div class="card">
                        <div id="headingTwo" class="card-header bg-white shadow-sm border-0">
                            <h2 class="mb-0">
                                <button type="button" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo" class="btn btn-link text-dark font-weight-bold text-uppercase collapsible-link collapsed">
                                    What is the difference between a textile printer and a fabric printer?
                                </button>
                            </h2>
                        </div>
                        <div id="collapseTwo" aria-labelledby="headingTwo" data-parent="#accordionExample" class="collapse" style="">
                            <div class="card-body p-5">
                                <p class="font-weight-light m-0">
                                    Both textile printers and fabric printers refer to machines that print on fabric, though industrial fabric printers are more commonly used in large production.
                                </p>
                            </div>
                        </div>
                    </div><!-- End -->
                    <!-- Accordion item 3 -->
                    <div class="card">
                        <div id="headingThree" class="card-header bg-white shadow-sm border-0">
                            <h2 class="mb-0">
                                <button type="button" data-toggle="collapse" data-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree" class="btn btn-link text-dark font-weight-bold text-uppercase collapsible-link collapsed">What are the types of textile printing machines available?</button>
                            </h2>
                        </div>
                        <div id="collapseThree" aria-labelledby="headingThree" data-parent="#accordionExample" class="collapse" style="">
                            <div class="card-body p-5">
                                <p class="font-weight-light m-0">Popular textile printing machines include digital textile printers, pigment-based digital fabric printers, high speed textile printers, and rotary machines.</p>
                            </div>
                        </div>
                    </div><!-- End -->
                    <div class="card">
                        <div id="headingFour" class="card-header bg-white shadow-sm border-0">
                            <h2 class="mb-0">
                                <button type="button" data-toggle="collapse" data-target="#collapseFour" aria-expanded="true" aria-controls="collapseThree" class="btn btn-link text-dark font-weight-bold text-uppercase collapsible-link">What is an industrial fabric printing machine?</button>
                            </h2>
                        </div>
                        <div id="collapseFour" aria-labelledby="headingFour" data-parent="#accordionExample" class="collapse " style="">
                            <div class="card-body p-5">
                                <p class="font-weight-light m-0">An industrial fabric printing machine is built for bulk printing on textiles using advanced technology like Ricoh Gen6 fabric printers and other large-scale setups.</p>
                            </div>
                        </div>
                    </div><!-- End -->

                    <div class="card">
                        <div id="headingFive" class="card-header bg-white shadow-sm border-0">
                            <h2 class="mb-0">
                                <button type="button" data-toggle="collapse" data-target="#collapseFive" aria-expanded="true" aria-controls="collapseThree" class="btn btn-link text-dark font-weight-bold text-uppercase collapsible-link">What is the cost of a fabric printing machine?</button>
                            </h2>
                        </div>
                        <div id="collapseFive" aria-labelledby="headingFive" data-parent="#accordionExample" class="collapse " style="">
                            <div class="card-body p-5">
                                <p class="font-weight-light m-0">The fabric printing machine cost depends on speed, print width, and ink type. Industrial fabric printers cost more than entry-level textile printing machines.</p>
                            </div>
                        </div>
                    </div><!-- End -->
                    <div class="card">
                        <div id="headingsix" class="card-header bg-white shadow-sm border-0">
                            <h2 class="mb-0">
                                <button type="button" data-toggle="collapse" data-target="#collapsesix" aria-expanded="true" aria-controls="collapseThree" class="btn btn-link text-dark font-weight-bold text-uppercase collapsible-link">What is pigment-based digital fabric printing?</button>
                            </h2>
                        </div>
                        <div id="collapsesix" aria-labelledby="headingsix" data-parent="#accordionExample" class="collapse " style="">
                            <div class="card-body p-5">
                                <p class="font-weight-light m-0">Pigment-based digital fabric printing uses pigment inks ideal for fabric textile printing on cotton and blends, offering excellent wash and rub fastness.</p>
                            </div>
                        </div>
                    </div><!-- End -->
                    <div class="card">
                        <div id="headingseven" class="card-header bg-white shadow-sm border-0">
                            <h2 class="mb-0">
                                <button type="button" data-toggle="collapse" data-target="#collapseseven" aria-expanded="true" aria-controls="collapseThree" class="btn btn-link text-dark font-weight-bold text-uppercase collapsible-link">Which is the best textile printer for high-speed production?</button>
                            </h2>
                        </div>
                        <div id="collapseseven" aria-labelledby="headingseven" data-parent="#accordionExample" class="collapse " style="">
                            <div class="card-body p-5">
                                <p class="font-weight-light m-0">The best textile printer for high-speed jobs is the high speed textile printer like the Ricoh Gen6 fabric printer designed for industrial-scale printing.</p>
                            </div>
                        </div>
                    </div><!-- End -->

                    <div class="card">
                        <div id="headingeight" class="card-header bg-white shadow-sm border-0">
                            <h2 class="mb-0">
                                <button type="button" data-toggle="collapse" data-target="#collapseeight" aria-expanded="true" aria-controls="collapseThree" class="btn btn-link text-dark font-weight-bold text-uppercase collapsible-link">Can I find textile printing machines near me?</button>
                            </h2>
                        </div>
                        <div id="collapseeight" aria-labelledby="headingeight" data-parent="#accordionExample" class="collapse " style="">
                            <div class="card-body p-5">
                                <p class="font-weight-light m-0">Yes, HGS Machines offers textile printing machines near me through a wide network of service and delivery options for fabric printing equipment.</p>
                            </div>
                        </div>
                    </div><!-- End -->
                    <div class="card">
                        <div id="headingnine" class="card-header bg-white shadow-sm border-0">
                            <h2 class="mb-0">
                                <button type="button" data-toggle="collapse" data-target="#collapsenine" aria-expanded="true" aria-controls="collapseThree" class="btn btn-link text-dark font-weight-bold text-uppercase collapsible-link">What fabrics can be printed using digital textile fabric printers?</button>
                            </h2>
                        </div>
                        <div id="collapsenine" aria-labelledby="headingnine" data-parent="#accordionExample" class="collapse " style="">
                            <div class="card-body p-5">
                                <p class="font-weight-light m-0">Digital textile fabric printing supports cotton, polyester, silk, and blended textiles using textile printers with pigment, reactive, or sublimation inks.</p>
                            </div>
                        </div>
                    </div><!-- End -->
                    <div class="card">
                        <div id="headingten" class="card-header bg-white shadow-sm border-0">
                            <h2 class="mb-0">
                                <button type="button" data-toggle="collapse" data-target="#collapseten" aria-expanded="true" aria-controls="collapseThree" class="btn btn-link text-dark font-weight-bold text-uppercase collapsible-link">What equipment is included in a fabric printing machine setup?</button>
                            </h2>
                        </div>
                        <div id="collapseten" aria-labelledby="headingten" data-parent="#accordionExample" class="collapse " style="">
                            <div class="card-body p-5">
                                <p class="font-weight-light m-0">A full fabric printing equipment setup includes a textile printing machine, RIP software, drying/fixation units, and pre-treatment systems for smooth operation.</p>
                            </div>
                        </div>
                    </div><!-- End -->
                    <!-- Accordion item 3 -->

                </div><!-- End -->
            </div>
        </div>
    </div>
</section>
<section class="request-quote-section" style="background-image: url(/images/background/6.jpg);">
    <div class="auto-container">
        <div class="sec-title light">
            <div class="title">We are HGS Machine</div>
            <h2>Request a Quote</h2>
            <div class="text">Looking for the perfect digital textile printing solution? Contact us today for a customized quote tailored to your needs!</div>
        </div>
        <div class="appointment-form">
            <form method="post" action="#">
                <div class="row clearfix">
                    <div class="field-group col-xl-8 col-lg-12 col-md-12 col-sm-12">
                        <div class="row clearfix">
                            <div class="form-group col-lg-6 col-md-6 col-sm-12">
                                <input type="text" name="field-name" placeholder="Your Name " required="">
                            </div>

                            <div class="form-group col-lg-6 col-md-6 col-sm-12">
                                <input type="email" name="field-name" placeholder="Email Address" required="">
                            </div>

                            <div class="form-group col-lg-6 col-md-6 col-sm-12">
                                <input type="text" name="field-name" placeholder="Subject" required="">
                            </div>

                            <div class="form-group col-lg-6 col-md-6 col-sm-12">
                                <select class="custom-select-box" id="ui-id-1" style="display: none;">
                                    <option>Product Interested in</option>
                                    <option>Blaze</option>
                                    <option>Sappahire</option>
                                    <option>Huegush</option>
                                    <option>Leopard</option>
                                    <option>Chrome</option>
                                    <option>Tejas</option>

                                </select><span tabindex="0" id="ui-id-1-button" role="combobox" aria-expanded="false" aria-autocomplete="list" aria-owns="ui-id-1-menu" aria-haspopup="true" class="ui-selectmenu-button ui-selectmenu-button-closed ui-corner-all ui-button ui-widget"><span class="ui-selectmenu-icon ui-icon ui-icon-triangle-1-s"></span><span class="ui-selectmenu-text">Product Interested in</span></span>

                            </div>
                        </div>
                    </div>
                    <div class="field-group col-xl-4 col-lg-12 col-md-12 col-sm-12">
                        <div class="row clearfix">
                            <div class="form-group col-lg-12 col-md-12 col-sm-12">
                                <textarea name="field-name" placeholder="Mesage " required=""></textarea>
                            </div>
                            <div class="form-group col-lg-12 col-md-12 col-sm-12">
                                <button class="theme-btn btn-style-one" type="submit">Submit</button>
                            </div>
                        </div>
                    </div>

                </div>
            </form>
        </div>
    </div>
</section>


